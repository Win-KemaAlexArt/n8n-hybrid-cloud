# ===========================================
# AWS НАСТРОЙКИ
# ===========================================

# AWS Credentials (НАСТРОИТЬ)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

# AWS EC2 настройки
AWS_INSTANCE_TYPE=t2.micro
AWS_KEY_PAIR_NAME=your_key_pair
AWS_SECURITY_GROUP_ID=sg-xxxxxxxxx

# AWS RDS настройки
AWS_RDS_ENDPOINT=your-rds-endpoint.amazonaws.com
AWS_RDS_USERNAME=n8n_user
AWS_RDS_PASSWORD=your_rds_password
AWS_RDS_DATABASE=n8n

# ===========================================
# ДОПОЛНИТЕЛЬНЫЕ СЕРВИСЫ
# ===========================================

# Redis (опционально)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Telegram Bot (если используется)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# Email настройки (SES/SMTP)
SMTP_HOST=email-smtp.us-east-1.amazonaws.com
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASSWORD=your_smtp_password
SMTP_FROM_EMAIL=noreply@your-domain.com

# ===========================================
# МОНИТОРИНГ И ЛОГИРОВАНИЕ
# ===========================================

# Уровень логирования
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console,file

# Очистка данных выполнения
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=168  # 7 дней
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all

# ===========================================
# БЕЗОПАСНОСТЬ
# ===========================================

# Encryption key для n8n
N8N_ENCRYPTION_KEY=your_32_character_encryption_key

# JWT секрет
JWT_SECRET=your_jwt_secret_key

# CORS настройки
N8N_CORS_ORIGIN=https://your-domain.com

# ===========================================
# РАЗРАБОТКА
# ===========================================

# Режим разработки
NODE_ENV=production
N8N_DIAGNOSTICS_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false

# Webhook тестирование
N8N_SKIP_WEBHOOK_DEREGISTRATION_SHUTDOWN=true
